{
  "name": "object-diagram-modeler-starter",
  "version": "0.0.0",
  "description": "A simple object diagram editor built with diagram-js",
  "scripts": {
    "all": "run-s lint build",
    "build": "webpack",
    "lint": "eslint ",
    "start": "run-s dev",
    "dev": "run-p build:watch serve",
    "build:watch": "webpack -w",
    "serve": "serve public",
    "build:github-pages": "webpack --env ghpages",

    "startDebugger": "run-s devDebugger",
    "devDebugger": "run-p buildDebugger:watch serve",
    "buildDebugger:watch": "webpack -w --env debuggerDev",
    "buildDebugger:deploy": "webpack --env debuggerDeploy"
  },
  "private": "true",
  "keywords": [
    "diagram-js",
    "object diagram editor",
    "object diagram modeler"
  ],
  "author": {
    "name": "Tim Kr√§uter",
    "url": "https://timkraeuter.com/"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.22.10",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.47.0",
    "eslint-plugin-bpmn-io": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^4.0.2",
    "serve": "^14.2.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "jquery": "^3.7.0",
    "object-diagram-modeler": "file:../"
  }
}
