{
  "name": "object-diagram-js-starter",
  "version": "0.0.0",
  "description": "A simple object diagram editor built with diagram-js",
  "scripts": {
    "all": "run-s lint pCheck build",
    "build": "webpack",
    "lint": "eslint .",
    "start": "run-s dev",
    "dev": "run-p build:watch serve",
    "build:watch": "webpack -w",
    "serve": "serve public",
    "build:github-pages": "rimraf .././docs && webpack --env ghpages",
    "pCheck": "prettier . --check",
    "pWrite": "prettier . --write",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "private": "true",
  "keywords": [
    "diagram-js",
    "object-diagram-js",
    "object diagram modeler"
  ],
  "author": {
    "name": "Tim Kr√§uter",
    "url": "https://timkraeuter.com/"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.28.6",
    "@playwright/test": "^1.58.2",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.8.1",
    "raw-loader": "^4.0.2",
    "rimraf": "^6.1.2",
    "serve": "^14.2.5",
    "style-loader": "^4.0.0",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "inherits": "^2.0.4",
    "object-diagram-js": "file:../"
  }
}
