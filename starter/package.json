{
  "name": "object-diagram-js-starter",
  "version": "0.0.0",
  "description": "A simple object diagram editor built with diagram-js",
  "scripts": {
    "all": "run-s lint pCheck build",
    "build": "vite build",
    "lint": "eslint .",
    "start": "run-s dev",
    "dev": "vite",
    "serve": "vite preview",
    "build:github-pages": "rimraf .././docs && GHPAGES=true vite build",
    "pCheck": "prettier . --check",
    "pWrite": "prettier . --write",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "private": "true",
  "keywords": [
    "diagram-js",
    "object-diagram-js",
    "object diagram modeler"
  ],
  "author": {
    "name": "Tim Kr√§uter",
    "url": "https://timkraeuter.com/"
  },
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "inherits": "^2.0.4",
    "object-diagram-js": "file:../"
  }
}
